                        <Font name="Franklin Gothic Heavy" size="18.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="idtxt" prefHeight="60.0" prefWidth="297.0" promptText="Identifiant" style="-fx-text-inner-color: gold;">
