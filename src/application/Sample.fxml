<StackPane prefHeight="500.0" prefWidth="500.0" styleClass="bodybg" stylesheets="@application.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
